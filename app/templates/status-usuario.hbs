 <div class="row">
    <div class="col s12">

    <div class="card">
        <div class="card-image waves-effect waves-block waves-light">
            <img class="activator" src="{{model.unidadHab.imageUrl}}">
        </div>
        <div class="card-content">
            <span class="card-title activator grey-text text-darken-4">
                {{model.nombre}} {{model.apellidoPaterno}} {{model.apellidoMaterno}}
                <i class="material-icons right">more_vert</i>
            </span>
            <p>
                {{#if model.status.isFulfilled}}
                    <a>
                        Status: {{model.status.content}}
                    </a>                             
                {{/if}}
            </p>
            </div>
            <div class="card-reveal">
                <span class="card-title grey-text text-darken-4">
                    <i class="material-icons right">close</i>
                    Datos Generales del Colono
                </span>
                <ul>
                    <li>
                        <p>Nombre Completo: {{model.nombre}} {{model.apellidoPaterno}} {{model.apellidoMaterno}}</p>
                    </li>
                    <li>
                        <p>Correo Electrónico: {{model.email}}</p>
                    </li>
                    <li>
                        <p>Dirección: {{model.direccion}}</p>
                    </li>
                    <li>
                        <p>Teléfono: {{model.telefono}}</p>
                    </li>
                    <li>
                        <p>Saldo de este mes: {{format-money model.currentPagos symbol=selectedCurrency format="%v %s"}}</p>
                    </li>
                </ul>
            </div>
          </div>
        </div>
    </div>

  <nav>
    <div class="nav-content">
        <ul class="tabs tabs-transparent">
            <li class="tab">
                {{#link-to "dashboard" class="active"}}
                  <i class="material-icons right">view_module</i>
                    Volver
               {{/link-to}}
            </li>
        </ul>
    </div>
</nav>